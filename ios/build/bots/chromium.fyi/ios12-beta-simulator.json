{
  "comments": [
    "Runs tests on 64-bit iOS 10 and 11 and 12 tests",
    "on iPad, iPhone, @3x, and @2x on main and CQ ios12-beta-simulator.",
    "Note: Xcode 10.0 requires Mac OS 10.13.X or higher, hence 'host os'."
  ],
  "xcode build version": "9q1019a",
  "gn_args": [
    "additional_target_cpus=[\"x86\"]",
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "symbol_level=1",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "additional_compile_targets": [
    "all"
  ],
  "configuration": "Debug",
  "sdk": "iphonesimulator12.0",
  "tests_ignore": [
    {
      "include": "eg_cq_tests.json",
      "device type": "iPhone X",
      "os": "12.0",
