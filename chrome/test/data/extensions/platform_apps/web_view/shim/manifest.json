{
  "name": "Platform App Test: <webview>",
  "manifest_version": 2,
  "version": "2",
  "permissions": [
    "webview"
  ],
  "app": {
    "background": {
      "scripts": ["test.js"]
    }
  },
  "webview": {
    "partitions": [
      {
        "name": "foobar",
        "accessible_resources": [
          "child_frame.html",
          "parent_frame.html",
          "guest_with_inline_script.html",
          "test.pdf",
          "guest_with_select.html"
        ]
      }
    ]
  }
}
