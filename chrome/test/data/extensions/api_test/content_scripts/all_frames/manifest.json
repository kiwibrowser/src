{
  "name": "content_script_all_frames",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tests the all_frames property of content script declarations.",
  "background": {
    "page": "test.html"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["top_frame_only.js"]
    },
    {
      "matches": ["http://*/*"],
      "js": ["all_frames.js"],
      "all_frames": true
    }
  ]
}
