{
  "name": "content_script_extension_injector",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tests extension that tried to inject content script in other extensions.",
  "background": { "scripts": ["background.js"] },
  "permissions": [ "*://*/*" ],
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_end",
      "matches": ["http://a.com/*"],
      "js": ["inject.js"]
    }
  ]
}
