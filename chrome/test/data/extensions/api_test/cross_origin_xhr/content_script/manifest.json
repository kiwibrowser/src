{
  "name": "cross origin xhr tests",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Sanity check that cross-origin XHR works for extension content scripts.",
  "background": {
    "page": "test.html"
  },
  "content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "http://localhost/*" ]
  } ],
  "permissions": ["http://a.com/", "http://*.b.com/", "ftp://127.0.0.1/*"]
}
