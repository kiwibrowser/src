{
  "version": "1.0.0.0",
  "manifest_version": 2,
  "name": "css_origin test",
  "description": "Test the css_origin property of insertCSS",
  "background": {
    "scripts": ["test.js"]
  },
  "permissions": ["tabs", "http://b.com/"],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["b.js"]
    }
  ]
}
