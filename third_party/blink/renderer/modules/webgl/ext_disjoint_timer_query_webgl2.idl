[
    DoNotCheckConstants,
    NoInterfaceObject
] interface EXTDisjointTimerQueryWebGL2 {
    const GLenum QUERY_COUNTER_BITS_EXT      = 0x8864;
    const GLenum TIME_ELAPSED_EXT            = 0x88BF;
    const GLenum TIMESTAMP_EXT               = 0x8E28;
    const GLenum GPU_DISJOINT_EXT            = 0x8FBB;
