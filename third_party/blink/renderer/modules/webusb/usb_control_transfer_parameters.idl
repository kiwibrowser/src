enum USBRequestType {
    "standard",
    "class",
    "vendor"
};

enum USBRecipient {
    "device",
    "interface",
    "endpoint",
    "other",
};

dictionary USBControlTransferParameters {
    required USBRequestType requestType;
    required USBRecipient recipient;
    required octet request;
    required unsigned short value;
    required unsigned short index;
};
