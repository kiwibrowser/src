    },
    {
      name: "font-variant",
    },
    {
      name: "font-weight",
    },
    {
      name: "height",
    },
    {
      name: "max-height",
    },
    {
      name: "max-width",
    },
    {
      name: "max-zoom",
    },
    {
      name: "min-height",
    },
    {
      name: "min-width",
    },
    {
      name: "min-zoom",
    },
    {
      name: "orientation",
    },
    {
      name: "src",
    },
    {
      name: "unicode-range",
    },
    {
      name: "user-zoom",
    },
    {
      name: "width",
    },
    {
      name: "zoom",
    },
  ],
