	case basic
		version 310 es
		desc "Same uniform struct in both shaders"
		values {
			uniform float val.a = 1.0;
			uniform float val.b = 2.0;
			output float out0 = 3.0;
		}
		vertex ""
			#version 310 es
			${VERTEX_DECLARATIONS}
			struct Struct {mediump float a; mediump float b;};
			uniform Struct val;
			out mediump float dummy;
			void main()
			{
				dummy = val.a + val.b;
				${VERTEX_OUTPUT}
			}
		""
		fragment ""
			#version 310 es
			precision mediump float;
			struct Struct {mediump float a; mediump float b;};
			uniform Struct val;
			in mediump float dummy;
			${FRAGMENT_DECLARATIONS}
			void main()
			{
				out0 = val.b + val.a;
				out0 = out0 + dummy;
				out0 = out0 - dummy;
				${FRAGMENT_OUTPUT}
			}
		""
	end

	case vertex_only
