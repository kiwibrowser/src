group basic "Basic Tests"

	case correct_phases
		version 330
		expect compile_fail
		both ""
			#version 330
			#define e +1
			${DECLARATIONS}
			void main()
			{
				mediump int n = 1e;
				${OUTPUT}
			}
		""
	end

	case invalid_identifier
		version 330
		expect compile_fail
		both ""
			#version 330
			#define e +1
			${DECLARATIONS}
			void main()
			{
				mediump int 1xyz = 1;
				${OUTPUT}
			}
		""
	end

	case null_directive
		version 330
		values { output float out0 = 0.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			#
		# // comment
	/*sfd*/		# /* */

			void main()
			{
				out0 = 0.0;
				${OUTPUT}
			}
		""
	end

	case invalid_directive
		version 330
		expect compile_fail
		both ""
			#version 330
			#defin AAA
			${DECLARATIONS}

			void main()
			{
				${OUTPUT}
			}
		""
	end

	case missing_identifier
		version 330
		expect compile_fail
		both ""
			#version 330
			#define
			${DECLARATIONS}

			void main()
			{
				${OUTPUT}
			}
		""
	end

	case empty_object
		version 330
		values { output float out0 = -1.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			# define VALUE

			void main()
			{
				out0 = VALUE - 1.0;
				${OUTPUT}
			}
		""
	end

	case empty_function
		version 330
		values { output float out0 = -1.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			# define VALUE(a)

			void main()
			{
				out0 = VALUE(2.0) - 1.0;
				${OUTPUT}
			}
		""
	end

	case empty_directive
		version 330
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			#

			void main()
			{
				out0 = 1.0;
				${OUTPUT}
			}
		""
	end

	case token_pasting_1
		version 330
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			#define A(X) var##X

			void main()
			{
				float varfoo = 1.0;
				float var = -1.0;
				out0 = A(foo);
				${OUTPUT}
			}
		""
	end

	case token_pasting_2
		version 330
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS}

			#define A(X, Y) var##X##Y

			void main()
			{
				float varfoo = 1.0;
				float varf = -1.0;
				out0 = A(f, oo);
				${OUTPUT}
			}
		""
	end

end # basic

group definitions "Symbol Definition Tests"

	case define_value_and_function
		version 330
		values { output float out0 = 6.0; }

		both ""
			#version 330
			precision mediump float;
			${DECLARATIONS:single-line}
			#	define		VALUE			(1.5 + 2.5)
			#	define		FUNCTION(__LINE__, b)	__LINE__+b

			void main()
			{
				out0 = FUNCTION(VALUE, ((0.2) + 1.8) );
				${OUTPUT}
			}
		""
	end

	case undefine_object_invalid_syntax
		version 330
		expect compile_fail
		both ""
			#version 330
			precision mediump float;
			#define		VAL			2.0
			#undef		VAL	sdflkjfds
			#define		VAL			1.0
			${DECLARATIONS}

			void main()
			{
				${POSITION_FRAG_COLOR} = vec4(VAL);
			}
		""
	end

	case undefine_function
		version 330
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			#define		FUNCTION(a,b) a+b
			#undef		FUNCTION
			#define		FUNCTION(a,b) a-b
			${DECLARATIONS}

			void main()
			{
				out0 = FUNCTION(3.0, 2.0);
				${OUTPUT}
			}
		""
	end

	case undefine_version
		version 330
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			#undef __VERSION__
			${DECLARATIONS}

			void main()
			{
			#if defined(__VERSION__)
				out0 = -1.0;
			#else
				out0 = 1.0;
			#endif
				${OUTPUT}
			}
		""
	end

	case undefine_core_profile
		version 330
		expect compile_fail
		values { output float out0 = 1.0; }
		both ""
			#version 330
			precision mediump float;
			#undef GL_core_profile
			${DECLARATIONS}

			void main()
			{
			#if defined(GL_core_profile)
				out0 = -1.0;
			#else
				out0 = 1.0;
			#endif
				${OUTPUT}
			}
		""
	end

end # definitions

