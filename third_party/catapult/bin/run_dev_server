import os
import sys

if __name__ == '__main__':
  catapult_path = os.path.abspath(os.path.join(os.path.dirname(__file__), '..'))
  sys.path.append(catapult_path)
  from catapult_build import dev_server
  sys.exit(dev_server.Main(sys.argv))
